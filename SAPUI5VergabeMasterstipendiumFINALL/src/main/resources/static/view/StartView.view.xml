<mvc:View controllerName="com.sap.masterstipendium.SAPUI5VergabeMasterstipendiumFrontEnd.controller.StartView" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m" xmlns:l="sap.ui.layout">

	<Shell>
		<App id="app">
			<l:HorizontalLayout class="sapUiContentPadding" >
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Ausbildungsabteilung"
				 press="press" width="100%">
				<TileContent>
					<ImageContent src="sap-icon://course-book" />
				</TileContent>
			</GenericTile>

			<GenericTile header="Ausbilder" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
				 press="press2" width="100%">
				<TileContent>
					<ImageContent src="sap-icon://course-book" />
				</TileContent>
			</GenericTile>
			
			<GenericTile header="Alignment-Meeting" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
				 press="press2" width="100%">
				<TileContent>
					<ImageContent src="sap-icon://course-book" />
				</TileContent>
			</GenericTile>
			</l:HorizontalLayout>
		</App>
		</Shell>

</mvc:View>